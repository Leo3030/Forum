<?php

namespace Forum\Bundle\HomeBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * UserQuestionMapping
 */
class UserQuestionMapping
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $uId;

    /**
     * @var integer
     */
    private $qId;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set uId
     *
     * @param integer $uId
     * @return UserQuestionMapping
     */
    public function setUId($uId)
    {
        $this->uId = $uId;

        return $this;
    }

    /**
     * Get uId
     *
     * @return integer 
     */
    public function getUId()
    {
        return $this->uId;
    }

    /**
     * Set qId
     *
     * @param integer $qId
     * @return UserQuestionMapping
     */
    public function setQId($qId)
    {
        $this->qId = $qId;

        return $this;
    }

    /**
     * Get qId
     *
     * @return integer 
     */
    public function getQId()
    {
        return $this->qId;
    }
}
